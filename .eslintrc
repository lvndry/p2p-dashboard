{
    "extends": "airbnb",
    "settings": {
        "react": {
            "version": "detect"
        }
    },	"env": {
		"browser": true,
		"jest": true,
		"es6": true
	},
	"rules": {
		"arrow-body-style": "off",
		"comma-dangle": "warn",
		"import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
		"import/order": ["error", {"groups": ["builtin", "external", "parent", "sibling", "index"], "newlines-between": "always-and-inside-groups" }],
		"import/prefer-default-export": "off",
		"indent": ["error", "tab", { "SwitchCase": 1 }],
		"jsx-quotes": ["error", "prefer-single"],
		"no-nested-ternary": "off",
		"no-param-reassign": "off",
		"no-plusplus": "off",
		"no-tabs": ["error", { "allowIndentationTabs": true }],
		"no-unused-vars": ["warn"],
		"react/destructuring-assignment": "off",
		"react/forbid-prop-types": ["off"],
		"react/jsx-first-prop-new-line": ["warn", "multiline"],
		"react/jsx-indent": ["warn", "tab"],
		"react/jsx-indent-props": ["warn", "tab"],
		"react/jsx-one-expression-per-line": ["error", { "allow": "single-child" }],
		"react/no-array-index-key": "warn",
		"react/default-props-match-prop-types": ["warn", { "allowRequiredDefaults": true }],
		"react/require-default-props": ["warn", { "forbidDefaultForRequired": false }]
	}
}
